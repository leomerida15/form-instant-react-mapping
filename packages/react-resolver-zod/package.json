    "version": "2.0.2",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "name": "@form-instant/react-resolver-zod",
    "author": {
        "name": "leomerida15",
        "email": "dimasmerida15@gmail.com",
